<!doctype html>
<html lang="bg">
  <head>
    <meta charset="utf-8">
    <title>The Social Makerspace</title>
    <link rel="stylesheet" href="/bundle.css">
  </head>
  <body>
    <header>
      <img src="" alt="The Social Makerspace" class="logo">
      {% assign navPages = collections.all | eleventyNavigation %}
      <nav><ul>
      {%- for entry in navPages %}
        <li{% if entry.url == 'index' %} class="currentPage">
          {{ entry.title }}
          {%  else %}>
          <a href="trikey.space{{ entry.url }}">{{ entry.title }}</a>
          {% endif %} 
        </li>
      {%- endfor %}
      </ul></nav>
    </header>
    <main>
      <h1>{{ pageTitle }}</h1>
      {{ content }}
    </main>
    <footer>Â© 2023 - 2024 Toni Marinov. All rights reserved.</footer>
  </body>
</html>